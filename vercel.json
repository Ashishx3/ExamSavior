{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "files": [
      "views/**/*",
      "public/**/*"
    ],
    
    "redirects": [
        {
          "source": "/(.*)",
          "destination": "https://www.examsavior.net/$1",
          "permanent": true
        }
      ]
    
    
  }
  