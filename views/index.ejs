<script> 
  // for tv sliding efect 

  document.addEventListener('DOMContentLoaded', function () {
  let currentIndex = 0;
  const slideCount = 4; // Adjust to your total number of slides
  const intervalTime = 2500; // normal delay in ms
  const extraDelay = 700; // extra 1 second

  function autoSlide() {
    let slideInput = document.getElementById(`cr-${currentIndex}`);
    if (slideInput) {
      slideInput.checked = true;
    }

    currentIndex++;
    if (currentIndex > slideCount) {
      currentIndex = 0;
    }

    // Set the next timeout with extra delay if at index 0
    const nextDelay = currentIndex === 0 ? intervalTime + extraDelay : intervalTime;
    setTimeout(autoSlide, nextDelay);
  }

  // Start the first call
  autoSlide();
});

</script>

