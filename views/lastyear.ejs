    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;600&display=swap');
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Poppins', sans-serif;
            background: #0d0d0d;
            color: #fff;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height: 100vh;
        }

        .container {
            display: flex;
            width: 90%;
            max-width: 1200px;
            height: 80vh;
            gap: 15px;
        }

        .year-section {
            flex: 1;
            background: linear-gradient(135deg, #1a1a1a, #292929);
            border-radius: 12px;
            padding: 20px;
            text-align: center;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            transition: all 0.3s ease-in-out;
            box-shadow: 0px 5px 15px rgba(255, 255, 255, 0.1);
            border: 2px solid transparent;
        }

        .year-section:hover {
            transform: translateY(-5px);
            border-color: #ffcc00;
            box-shadow: 0px 5px 20px rgba(255, 204, 0, 0.5);
        }

        .year-title {
            font-size: 22px;
            font-weight: 600;
            color: #ffcc00;
            margin-bottom: 10px;
        }

        .papers-list {
            list-style: none;
            padding: 0;
            display: flex;
            flex-direction: column;
            gap: 10px;
        }

        .papers-list li {
            background: #ffcc00;
            color: #121212;
            padding: 12px 15px;
            border-radius: 5px;
            font-weight: bold;
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .papers-list li:hover {
            background: #e6b800;
            transform: scale(1.05);
        }

        @media (max-width: 768px) {
            .container {
                flex-direction: column;
                height: auto;
            }
        }
    </style>
</head>
