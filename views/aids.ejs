<style>
:root {
  /* Light theme colors */
  --bg: #f9fafb;
  --muted: #64748b;
  --text: #334155;
  --accent: #2563eb;
  --ring: #3b82f6;
  --card-bg: #ffffff;
  --card-border: rgba(148, 163, 184, 0.25);
  --link-bg: rgba(0, 0, 0, 0.02);
  --link-hover-bg: rgba(37, 99, 235, 0.1);
  --radius: 14px;
  --gap: 2rem;
}

.dark-theme {
  /* Dark theme overrides */
  --bg: #0f172a;
  --muted: #94a3b8;
  --text: #d1d5db;
  --accent: #60a5fa;
  --ring: #93c5fd;
  --card-bg: rgba(255,255,255,0.05);
  --card-border: rgba(148, 163, 184, 0.18);
  --link-bg: rgba(255,255,255,0.05);
  --link-hover-bg: rgba(96,165,250,.15);
}

body {
  background: var(--bg);
  color: var(--text);
  font-family: system-ui, sans-serif;
}

.wrap {
  width: 100%;
  padding: 24px 16px 48px;
  max-width: 1100px;
  margin: 0 auto;
}

.title {
  font-size: clamp(1.2rem, 2vw + 1rem, 1.8rem);
  margin: 0 0 12px;
  font-weight: 700;
  color: var(--accent);
}

.subtitle {
  color: var(--muted);
  margin: 0 0 20px;
  font-size: 1.25rem;
}

.grid {
  display: grid;
  grid-template-columns: 1fr;
  gap: var(--gap);
}
@media (min-width: 640px) {
  .grid { grid-template-columns: repeat(2, 1fr); }
}
@media (min-width: 1024px) {
  .grid { grid-template-columns: repeat(4, 1fr); }
}

.col {
  background: var(--card-bg);
  border: 1px solid var(--card-border);
  border-radius: var(--radius);
  padding: 16px;
  box-shadow: 0 6px 18px rgba(0,0,0,0.1);
}

.col h2 {
  color: var(--text);
  margin: 0 0 10px;
  font-size: 1rem;
  font-weight: 600;
}

.links { list-style: none; padding: 0; margin: 0; }
.links li + li { margin-top: 6px; }

.link {
  display: block;
  text-decoration: none;
  color: var(--text);
  padding: 10px 12px;
  border-radius: 10px;
  font-size: 0.9rem;
  border: 1px solid transparent;
  background: var(--link-bg);
  transition: transform .2s ease, border-color .2s ease, box-shadow .2s ease, background .2s ease;
}

.link small {
  display: block;
  color: var(--muted);
  font-size: .75rem;
}

.link:hover {
  transform: translateY(-1px);
  border-color: rgba(96, 165, 250, .45);
  box-shadow: 0 4px 10px rgba(59,130,246,.15);
  background: var(--link-hover-bg);
}

.link:focus-visible {
  outline: none;
  box-shadow: 0 0 0 2px var(--ring);
}

@media (max-width: 640px) {
  .wrap {
    padding: 12px;
    max-width: 420px;
    margin: 0 auto;
    text-align: center;
  }

  .title, 
  .subtitle {
    text-align: center;
  }

  .grid {
    margin-left: 36px;
    grid-template-columns: 1fr;
    gap: 0.75rem;           /* thoda gap kam */
    justify-items: center;  /* boxes center me */
  }

  .col {
    width: 92%;             /* full stretch na ho */
    max-width: 300px;       /* box chhota aur neat */
    padding: 10px 12px;     /* andar ka spacing chhota */
    border-radius: 10px;    /* thoda soft curve */
    box-shadow: 0 3px 8px rgba(0,0,0,0.08); /* halka sa shadow */
  }

  .col h2 {
    font-size: 0.9rem;      /* headings chhoti */
    margin-bottom: 6px;
  }

  .link {
    font-size: 0.8rem;      /* links chhoti */
    padding: 7px 9px;
  }
}



