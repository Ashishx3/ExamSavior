<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>Meet The Team</title>
  <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap" rel="stylesheet">
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Inter:ital,opsz,wght@0,14..32,100..900;1,14..32,100..900&display=swap');
    @import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap");

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body, html {
      height: 100%;
      width: 100%;
      overflow: hidden;
      font-family: 'Bebas Neue', sans-serif;
    }

    .video-bg {
      
      position: fixed;
      top: 0;
      left: 0;
      width: 100vw;
      height: 100vh;
      object-fit: cover;
      z-index: -1;
    }

    .text-wrapper {
      position: relative;
      padding: 40px;
      text-align: center;
    }

    .text-wrapper div {
    
      --s: 6px;
      --o: calc(0.5 * var(--s));
      --sl: calc(50% - 0.5px), #0000 calc(50% + 0.5px);

      font-size: clamp(3em, 10vw, 10em);
      text-transform: uppercase;
      color: transparent;
      -webkit-text-stroke: #fff var(--o);
      mix-blend-mode: screen;
      line-height: 1;

      background: 
        linear-gradient(22.5deg, #fff var(--sl)) text,
        linear-gradient(22.5deg, #000 var(--sl)),
        linear-gradient(#094770 50%, #0000 0) 0 0 / 1% var(--s);

      background-clip: text;
      -webkit-background-clip: text;
    }
    .memcards{
      border-radius: 9px;
      width: 330px;
      height: 120px;
      border: 1px solid rgb(46, 48, 50);
      padding: 4px;
      background-color: rgb(31, 31, 31);
      color: white;
      display: flex;
    }
    .position1{
      font-family: "Inter", serif;
  margin-left: 10px;
  margin-top: 12px;
  font-size: 18px;
  font-weight: 600;
  letter-spacing: -0.03em ;
  overflow-wrap: break-word;
  flex: 1; 
  align-items: self-start; /* center text vertically */
}
    .position {
      font-family: "Inter", serif;
  margin-left: 10px;
  letter-spacing: -0.03em ;
  flex: 1; 
  font-weight: 500;
  align-items: self-start; 
  color: rgba(94, 91, 91, 0.681);
  font-size: 16px;
  margin-top: 6px;
}

    section{
      padding: 36px;
      gap: 40px;
      display: flex;
      justify-content: center;
      
      flex-direction: row;
      flex-wrap: wrap;
    }
    .ttdiv{
      display: flex;
      justify-content: center;
      align-content: center;
    }
    .tranparent-div {
      text-align: center;
  background-color: rgba(31, 31, 31, 0.5); /* black with 50% transparency */

  width: 65%;
  border: 1px solid black;
  height: 200px;
  color: white;
      font-family: "poppins", serif;
      font-size: 30px;
}
.photos{
  object-fit: cover;
  width: 34%;
  height: 100%;
  border: 1px solid white;
  border-radius: 9px;
}
    
  </style>
</head>
<body>

  <!-- Background Video -->
   
  <video class="video-bg" autoplay muted loop playsinline>
    <source src="/images/vid4.mp4" type="video/mp4">
